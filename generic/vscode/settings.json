{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "window.zoomLevel": 0,
    "python.analysis.memory.keepLibraryAst": true,
    "python.analysis.memory.keepLibraryLocalVariables": true,
    "hdinsight.disableOpenSurveyLink": true,
    "workbench.activityBar.visible": true,
    "workbench.sideBar.location": "left",
    "makefileCommandRunner.makefilePath": "FILE_PATH",
    "makefileCommandRunner.integrationTestCommand": "all",
    "makefileCommandRunner.integrationTestWatchCommand": "all",
    "python.pythonPath": "/home/pyoung/venv/python3/bin/python",
    "python.linting.pylintPath": "/home/pyoung/venv/python3/bin/pylint",
    
    "terminal.integrated.automationShell.linux": "/bin/bash",
    "terminal.integrated.shellArgs.linux": [ "-l" ],
    
    "workbench.colorTheme": "Dracula",
    "workbench.startupEditor": "newUntitledFile",

    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    }

}


