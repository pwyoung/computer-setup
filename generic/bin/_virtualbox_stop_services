#!/bin/bash

#SVC=$(systemctl  | grep -i vbox | awk '{print $1}')
SVC='vboxautostart-service.service vboxballoonctrl-service.service vboxdrv.service vboxweb-service.service'
for i in $SVC; do
    echo "stop service $i"
    sudo systemctl stop $i
#    echo "start service $i"
#    sudo systemctl start $i
    echo "status service $i"
    sudo systemctl status $i    
done    